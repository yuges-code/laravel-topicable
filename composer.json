{
    "name": "yuges-code/laravel-topicable",
    "description": "ðŸ’¡Package for easily attaching topics to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "topics",
        "laravel",
        "package"
    ],
    "homepage": "https://yuges-code.github.io/laravel-topicable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-topicable",
        "issues": "https://github.com/yuges-code/laravel-topicable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "yuges-code/laravel-package": "^1.0",
        "yuges-code/laravel-sluggable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Topicable\\": "src",
            "Yuges\\Topicable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Topicable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Topicable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Topicable\\Providers\\TopicableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
